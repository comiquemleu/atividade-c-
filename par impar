#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{// inicio
    int i, numero[5];
    int impar=0,par=0;


    // var
    for (i = 0; i < 5; i++)
    {
        printf("digite um numero: ");
        scanf("%d", &numero[i]);

    if (numero[i] % 2 == 0)
    {
        par++;
    }
    else
    {
        impar++;

    }
    }
   printf("numeros pares:%d\n",par);
   printf("numeros impares:%d\n",impar);
    
    return 0;
}
